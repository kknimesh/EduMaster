# Production Environment Variables
NODE_ENV=production
PORT=8000

# AWS Configuration
AWS_REGION=us-east-1
AWS_SECRETS_MANAGER_SECRET_ID=edumaster/database

# Database (will be populated from AWS Secrets Manager)
DATABASE_URL=postgresql://username:password@host:5432/database

# JWT Configuration
JWT_SECRET_KEY=${JWT_SECRET_KEY}
JWT_EXPIRES_IN=24h

# Redis Configuration
REDIS_URL=${REDIS_URL}

# CORS Configuration
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info

# Health Check
HEALTH_CHECK_ENDPOINT=/health